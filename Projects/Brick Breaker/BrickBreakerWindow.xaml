<Window x:Class="GameCenterProject.Projects.Brick_Breaker.BrickBreakerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameCenterProject.Projects.Brick_Breaker"
        mc:Ignorable="d"
        Title="BrickBreakerWindow" Width="800" Height="600"
        WindowState="Maximized">
    <Grid>
        <Canvas Name="canvas" Background="LightGray" RenderTransformOrigin="0.504,0.566">
            <Rectangle HorizontalAlignment="Left" Height="35" Stroke="Teal" StrokeThickness="4" Fill="LightCyan" RadiusX="10" RadiusY="10" VerticalAlignment="Top" Width="200" Margin="0, 0, 0, 0"/>
        </Canvas>
    </Grid>
</Window>
